@model CloudKids.Domain.Entities.Reputation

@{
    ViewBag.Title = "Delete";
}
<br /><br /><br /><br /><br /><br /><br /><br />
<h2>Delete</h2>


<div>

    <hr />




    <section class="pager-sec">
        <div class="container">
            <div class="pager-pag">
                
                <h4>Are you sure you want to delete this Reputation ?</h4>
            <br />

                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="form-actions no-color">
                        <input type="submit" value="Delete" class="btn btn-default" /> 
                        @Html.ActionLink("Back To List", "Index", new { @class = "btn btn-default btn-green" })
                    </div>
                }
            </div><!--pager-pag end-->
        </div>
    </section><!--pager-sec end-->


  
</div>


