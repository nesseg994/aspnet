@model IEnumerable<CloudKids.Domain.Entities.Reclamation>
@Html.DropDownList("ProfilParentId", (IEnumerable<SelectListItem>)ViewBag.Profil_Parent, htmlAttributes: new { @class = "form-control" })

@{
    ViewBag.Title = "Index";
}
<br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
<section class="block2">
    <div class="fixed-bg bg-4"></div>
    <div class="container">
        <div class="descp-sec">
            <h3>List of all  <span>‘claims  ’</span>about gardens</h3>
        </div><!--descp-sec end-->
    </div>
</section>








<br /><br />
<div class="form-group">

    @using (Html.BeginForm("Myreclmation", "Reclamation", FormMethod.Post))
    {

        <div class="form-group">

            <div class="col-md-10">
              
    
                
                @Html.DropDownList("ProfilJardinId", (IEnumerable<SelectListItem>)ViewBag.Profil_Jardin, htmlAttributes: new { @class = "form-control" })


            </div>
        </div>

        <button id="ahmed1" type="submit"><i class="fa fa-search"></i></button>
    }
</div>

<div class="blog-post-info">
    <br /> <br />
</div>
<br />

@if (Model != null)
{
    foreach (var item in Model)
    {





        <div class="post-blog">
            <h3 class="post-title"><a href="blog-details.html" title="">@Html.DisplayFor(modelItem => item.ProfilJardin.Name)</a></h3>

            <span class="post-date"><i class="fa fa-picture-o"></i>@Html.DisplayFor(modelItem => item.ProfilParent.Name)</span>
            <div class="blog-post-info">
                <ul class="post-tg">
                    <li class="ad-author">
                        <span>By</span>
                        <a href="#" title="" class="ad-name">@Html.DisplayFor(modelItem => item.ProfilParent.Name)</a>
                    </li>

                    <li>
                        <i class="fa fa-tags"></i>
                        <a href="#" title="">@Html.DisplayFor(modelItem => item.catreclamation)</a>

                    </li>
                </ul>
                <p> @Html.DisplayFor(modelItem => item.Motif) </p>


                @Html.ActionLink("Delete", "Delete", new { id = item.Id }, new { @class = "btn btn-default btn-green" })
            </div><!--blog-post-info end-->
        </div><!--post-blog end-->






    }

}
   
