        /* Slideshow */
        /*img {vertical-align: middle;}*/

        .slideshow-container img.inactive {
            position:absolute;
            width:700px;
            height:auto;
            top:0;
            left:0;
            z-index:8;
            opacity:0.0;
            transition: opacity 1s;
        }

        .slideshow-container img.active {
            z-index:10;
        }
        .slideshow-container img.last-active {
            z-index:9;
        }

        /* Slideshow container */
        .slideshow-container {
            max-width: 1000px;
            position: relative;
            margin: auto;
            clear:both;
            margin-left: 60px;
        }

        /* Caption text */
        .text {
            color: #f2f2f2;
            font-size: 15px;
            padding: 8px 12px;
            position: absolute;
            bottom: 8px;
            width: 100%;
            text-align: center;
        }

        /* Number text (1/3 etc) */
        .numbertext {
            color: #f2f2f2;
            font-size: 12px;
            padding: 8px 12px;
            position: absolute;
            top: 0;
        }

        .numbertext.inactive {
            display: none;
        }

        /* The dots/bullets/indicators */
        .dot {
            height: 15px;
            width: 15px;
            margin: 0 2px;
            background-color: #bbb;
            border-radius: 50%;
            display: inline-block;
            transition: background-color 0.6s ease;
        }

        .dot.active {
            background-color: darkslategray;
        }

        /*.active {
